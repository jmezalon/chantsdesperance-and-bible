{
  "expo": {
    "name": "Chants d'Esp√©rance & Bible",
    "slug": "chants-esperance-bible",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "chantsesperance",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "jmezalon",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chantsesperance.bible",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app does not use the microphone.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFBF5",
        "foregroundImage": "./assets/images/icon.png"
      },
      "package": "com.chantsesperance.bible",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFBF5",
          "dark": {
            "backgroundColor": "#1A1512"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "9dc18771-91ab-4e4c-979b-3602237118ca"
      }
    }
  }
}
